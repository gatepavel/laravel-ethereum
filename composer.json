{
    "name": "gatepavel/laravel-ethereum",
    "description": "the ethereum json rpc api",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Ethereum",
        "Laravel"
    ],
    "authors": [
        {
            "name": "lee",
            "email": "jcsoft@aliyun.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Jcsofts\\LaravelEthereum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jcsofts\\LaravelEthereum\\EthereumServiceProvider"
            ],
            "aliases": {
                "Ethereum": "Jcsofts\\LaravelEthereum\\Facade\\Ethereum"
            }
        }
    }
}
